<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.image.ImageView?>

<fx:root xmlns:fx="http://javafx.com/fxml/1" type="VBox">
	<children>
		<Button fx:id="mapButton" text="Map"/>
		<Button fx:id="graphButton" text="Graph"/>
		<Button fx:id="listRatDataButton" text="Rat Data"/>
		<Button fx:id="profileButton" text="Profile"/>
		<Button fx:id="settingsButton" text="Settings"/>
		<Button fx:id="reportRatButton" text="Report a Rat"/>
		<Button fx:id="logoutButton" text="Logout"/>
		<Button fx:id="filterButton" text="Edit Filters"/>
		<Pane VBox.vgrow="ALWAYS"/>
		<ImageView fx:id="imageView"/>
	</children>
</fx:root>

